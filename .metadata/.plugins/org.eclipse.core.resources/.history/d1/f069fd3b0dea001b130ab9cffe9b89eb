package com.example;

public class String {
	
	private static String Palindrome(String a) {
		boolean result = true;
		int l=a.length();
		for(int i=0; i < len/2; i++) {
			if(a.charAt(i) != a.charAt(l-i-1)) {
				result = false;
				break;
			}
		}
		System.out.println(a + " is palindrome = "+result);
		
	}

}
