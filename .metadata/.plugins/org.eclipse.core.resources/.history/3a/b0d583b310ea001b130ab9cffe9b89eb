package com.example;

import java.util.Scanner;

public class Average {
	
	public static double average() {
		Scanner sc=new Scanner(System.in);
		System.out.print("Enter N: ");
		int N=sc.nextInt();
		System.out.println("Enter "+N+ "numbers: ");
		
		for(int i=0;i<N;i++) {
			int num=sc.nextInt();
			int sum=0;
			sum=sum+num;
			
		}
		
		
		
		
		
	}

}
